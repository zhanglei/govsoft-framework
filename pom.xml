	<ciManagement>
		<system>Continuum</system>
		<url>http://localhost:8080/continuum</url>
		<notifiers>
			<notifier>
				<type>mail</type>
				<address>zhanglei_job_email@163.com</address>
			</notifier>
		</notifiers>
	</ciManagement>
</project>
